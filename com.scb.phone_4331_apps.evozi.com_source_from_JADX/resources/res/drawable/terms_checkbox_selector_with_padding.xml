<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item state_checked="false">
        <layer-list>
            <item top="4dp">
                <bitmap gravity="bottom" src="@drawable/ic_terms_unchecked_gray"/>
            </item>
        </layer-list>
    </item>
    <item state_checked="true">
        <layer-list>
            <item top="4dp">
                <bitmap gravity="bottom" src="@drawable/ic_terms_checked"/>
            </item>
        </layer-list>
    </item>
</selector>
