<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" orientation="vertical" id="@+id/root" background="#f5f3f6" layout_width="match_parent" layout_height="match_parent">
    <TextView id="@+id/additional_info_text" layout_width="match_parent" layout_height="wrap_content" layout_marginTop="32dp" layout_marginBottom="32dp" text="@string/registration_manage_device" textAlignment="center" layout_marginStart="24dp" layout_marginEnd="24dp" style="@style/2131820992"/>
    <androidx.recyclerview.widget.RecyclerView id="@+id/device_settings_recycler_view" background="#f5f3f6" layout_width="match_parent" layout_height="match_parent" layout_above="@+id/next_button" layout_below="@+id/additional_info_text"/>
    <Button enabled="false" gravity="center" id="@+id/next_button" layout_width="match_parent" layout_height="wrap_content" layout_marginTop="16dp" layout_marginBottom="26dp" text="@string/next" layout_alignParentBottom="true" layout_marginStart="16dp" layout_marginEnd="16dp" style="@style/2131820837"/>
</RelativeLayout>
