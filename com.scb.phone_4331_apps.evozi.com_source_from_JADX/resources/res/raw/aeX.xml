<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" orientation="vertical" background="#f5f3f6" layout_width="match_parent" layout_height="match_parent">
    <include id="@+id/toolbar" layout="@layout/2131494467"/>
    <LinearLayout orientation="vertical" id="@+id/ll_search_container" paddingBottom="16dp" layout_width="match_parent" layout_height="wrap_content">
        <TextView ellipsize="end" id="@+id/tv_search_by" paddingTop="8dp" visibility="gone" layout_width="match_parent" layout_height="wrap_content" maxLines="1" paddingStart="16dp" paddingEnd="16dp" style="@style/2131821098"/>
        <LinearLayout orientation="horizontal" id="@+id/ll_search_items" paddingTop="8dp" visibility="gone" layout_width="match_parent" layout_height="wrap_content" paddingStart="16dp" paddingEnd="16dp">
            <View layout_gravity="center_vertical" layout_width="0dp" layout_weight="1" style="@style/2131821259"/>
            <TextView id="@+id/tv_search_items" layout_width="wrap_content" layout_height="wrap_content" paddingStart="16dp" paddingEnd="16dp" style="@style/2131821060"/>
            <View layout_gravity="center_vertical" layout_width="0dp" layout_weight="1" style="@style/2131821259"/>
        </LinearLayout>
    </LinearLayout>
    <fragment name="com.scb.phone.view.fragment.ccrredemption.product.ProductListFragment" id="@+id/fragment_category_listing" layout_width="match_parent" layout_height="match_parent"/>
</LinearLayout>
