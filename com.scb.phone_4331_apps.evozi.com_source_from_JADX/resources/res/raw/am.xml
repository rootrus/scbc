<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item state_enabled="false" color="?attr/colorControlNormal" alpha="?android:attr/disabledAlpha"/>
    <item state_focused="true" color="?attr/colorControlActivated"/>
    <item state_pressed="true" color="?attr/colorControlActivated"/>
    <item color="?attr/colorControlActivated" state_activated="true"/>
    <item state_selected="true" color="?attr/colorControlActivated"/>
    <item state_checked="true" color="?attr/colorControlActivated"/>
    <item color="?attr/colorControlNormal"/>
</selector>
