<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" orientation="vertical" paddingTop="16dp" layout_width="match_parent" layout_height="wrap_content">
    <LinearLayout orientation="horizontal" id="@+id/effective_layout" layout_width="match_parent" layout_height="wrap_content">
        <TextView layout_gravity="center_vertical" id="@+id/effective_label" layout_width="0dp" layout_height="wrap_content" layout_weight="1" textAllCaps="true" paddingStart="16dp" style="@style/2131821012"/>
        <LinearLayout gravity="end" layout_gravity="end" id="@+id/effective_info_layout" layout_width="wrap_content" layout_height="wrap_content" paddingEnd="16dp">
            <ImageView layout_gravity="center_vertical" id="@+id/effective_icon" layout_width="20dp" layout_height="20dp" src="@drawable/calendar_schedule_icon"/>
            <TextView ellipsize="end" gravity="end" layout_gravity="center_vertical" id="@+id/effective_text" layout_width="wrap_content" layout_height="wrap_content" maxWidth="@dimen/2131165912" layout_marginStart="8dp" style="@style/2131821020"/>
        </LinearLayout>
    </LinearLayout>
    <TextView ellipsize="end" layout_gravity="end" id="@+id/effective_description" visibility="gone" layout_width="wrap_content" layout_height="wrap_content" layout_marginStart="8dp" layout_marginEnd="16dp" style="@style/2131820994"/>
    <LinearLayout orientation="horizontal" id="@+id/transaction_layout" visibility="gone" layout_width="match_parent" layout_height="wrap_content">
        <TextView layout_gravity="center_vertical" id="@+id/transaction_label" layout_width="0dp" layout_height="wrap_content" layout_weight="1" textAllCaps="true" layout_marginStart="16dp" style="@style/2131821012"/>
        <TextView ellipsize="end" layout_gravity="top|right|center_vertical|center_horizontal|center|end" id="@+id/transaction_text" layout_width="wrap_content" layout_height="wrap_content" maxWidth="@dimen/2131165912" layout_marginStart="8dp" layout_marginEnd="16dp" style="@style/2131821020"/>
    </LinearLayout>
    <androidx.legacy.widget.Space layout_width="match_parent" layout_height="16dp"/>
    <View id="@+id/divider" layout_marginStart="16dp" style="@style/2131821259"/>
</LinearLayout>
