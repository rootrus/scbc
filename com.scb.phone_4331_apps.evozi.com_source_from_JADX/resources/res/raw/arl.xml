<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" id="@+id/root_view_successful" background="#f5f3f6" descendantFocusability="blocksDescendants" layout_width="match_parent" layout_height="match_parent">
