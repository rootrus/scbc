        <ImageView id="@+id/ic_arrow_right_coachmark" layout_width="wrap_content" layout_height="wrap_content" layout_marginBottom="8dp" src="@drawable/ic_arrow_right_coachmark" layout_below="@+id/qr_code_section_coachmark_description" layout_alignParentRight="true" contentDescription="@null" scaleY="-1" layout_marginEnd="48dp" layout_alignParentEnd="true"/>
