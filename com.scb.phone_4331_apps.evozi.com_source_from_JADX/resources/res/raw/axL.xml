                <TextView id="@+id/empty_description_text_view" paddingTop="3dp" paddingBottom="16dp" layout_width="wrap_content" layout_height="wrap_content" text="sdfsdfds d fdsfd" layout_below="@+id/empty_title_text_view" paddingStart="16dp" paddingEnd="0dp" style="@style/2131821054"/>
                <ImageView id="@+id/empty_button_image_view" padding="10dp" layout_width="wrap_content" layout_height="wrap_content" src="@drawable/ic_reload_empty_item" layout_alignParentRight="true" layout_centerVertical="true" layout_alignParentEnd="true"/>
                <View background="@drawable/ripple_purple" layout_width="match_parent" layout_height="wrap_content" layout_alignTop="@+id/empty_title_text_view" layout_alignBottom="@+id/empty_description_text_view"/>
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
