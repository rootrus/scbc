<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" layout_width="match_parent" layout_height="match_parent">
    <TextView id="@+id/title_text" layout_width="match_parent" layout_height="wrap_content" layout_marginTop="16dp" text="@string/note" layout_marginStart="16dp" layout_marginEnd="16dp" style="@style/2131821015"/>
