<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" orientation="vertical" background="#ffffff" layout_width="match_parent" layout_height="match_parent">
    <TextView id="@+id/tv_dialog_title" padding="16dp" layout_width="match_parent" layout_height="wrap_content" style="@style/2131821098"/>
    <View background="#7631c1" layout_width="match_parent" layout_height="1dp"/>
    <LinearLayout orientation="horizontal" layout_width="match_parent" layout_height="wrap_content" layout_margin="16dp">
        <com.scb.phone.view.custom.ColorChangeableNumberPicker id="@+id/month_picker" descendantFocusability="blocksDescendants" layout_width="wrap_content" layout_height="wrap_content" layout_weight="1" layout_marginEnd="16dp" NOT_FOUND_0xf="@color/2131099881"/>
        <com.scb.phone.view.custom.ColorChangeableNumberPicker id="@+id/year_picker" descendantFocusability="blocksDescendants" layout_width="wrap_content" layout_height="wrap_content" layout_weight="1" layout_marginStart="16dp" NOT_FOUND_0xf="@color/2131099881"/>
    </LinearLayout>
    <View background="#959199" layout_width="match_parent" layout_height="1dp" layout_marginTop="16dp"/>
    <Button gravity="center" layout_gravity="center_horizontal" id="@+id/bt_done" background="@null" padding="8dp" layout_width="match_parent" layout_height="wrap_content" text="@string/done" style="@style/2131821013"/>
</LinearLayout>
