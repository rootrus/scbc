<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" orientation="vertical" layout_width="match_parent" layout_height="wrap_content">
    <RelativeLayout layout_width="match_parent" layout_height="wrap_content" layout_marginTop="14dp">
        <TextView id="@+id/title" layout_width="wrap_content" layout_height="wrap_content" layout_alignParentLeft="true" layout_alignParentTop="true" layout_marginStart="12dp" layout_marginEnd="16dp" layout_toStartOf="@+id/navigation_view" layout_toEndOf="@+id/checkbox" layout_alignParentStart="true" style="@style/2131820985"/>
        <LinearLayout orientation="horizontal" id="@+id/navigation_view" layout_width="wrap_content" layout_height="wrap_content" layout_alignParentRight="true" layout_marginEnd="12dp" layout_alignParentEnd="true">
            <TextView textStyle="bold" id="@+id/link_text_view" layout_width="wrap_content" layout_height="wrap_content" style="@style/2131821094"/>
            <TextView layout_width="wrap_content" layout_height="wrap_content" text=" &gt;" style="@style/2131821101"/>
        </LinearLayout>
    </RelativeLayout>
    <RadioGroup orientation="horizontal" id="@+id/radio_group" layout_width="match_parent" layout_height="wrap_content" layout_marginStart="16dp" style="@style/2131821101">
        <RadioButton id="@+id/radio_accept" paddingTop="12dp" paddingBottom="16dp" layout_width="wrap_content" layout_height="wrap_content" checked="false" text="@string/accept_radio" style="@style/2131820985"/>
        <RadioButton id="@+id/radio_decline" paddingTop="12dp" paddingBottom="16dp" layout_width="wrap_content" layout_height="wrap_content" checked="false" text="@string/decline_radio" layout_marginStart="80dp" style="@style/2131820985"/>
    </RadioGroup>
</LinearLayout>
