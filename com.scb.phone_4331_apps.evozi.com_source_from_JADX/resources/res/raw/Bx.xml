<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt">
    <item state_checked="true" id="@+id/visible" drawable="@drawable/design_ic_visibility"/>
    <item id="@+id/masked" drawable="@drawable/design_ic_visibility_off"/>
    <transition drawable="@drawable/avd_show_password" toId="@+id/visible" fromId="@+id/masked"/>
    <transition drawable="@drawable/avd_hide_password" toId="@+id/masked" fromId="@+id/visible"/>
</animated-selector>
