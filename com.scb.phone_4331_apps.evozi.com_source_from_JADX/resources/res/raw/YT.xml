        <ImageView layout_gravity="center" id="@+id/submenuarrow" visibility="gone" layout_width="wrap_content" layout_height="wrap_content" scaleType="center" style="@style/2131820847"/>
    </LinearLayout>
</androidx.appcompat.view.menu.ListMenuItemView>
