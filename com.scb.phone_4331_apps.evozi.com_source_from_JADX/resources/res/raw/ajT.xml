<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" clickable="true" layout_width="match_parent" layout_height="wrap_content">
    <RelativeLayout id="@+id/biller_grid_layout" background="@drawable/ripple_purple" layout_width="match_parent" layout_height="wrap_content">
        <ImageView id="@+id/biller_avatar_image_view" layout_width="50dp" layout_height="50dp" layout_marginTop="8dp" scaleType="fitCenter" layout_centerHorizontal="true" contentDescription="@null"/>
        <TextView ellipsize="end" gravity="center" id="@+id/biller_name_text_view" paddingBottom="6dp" layout_width="wrap_content" layout_height="wrap_content" layout_marginTop="6dp" maxLines="2" minLines="2" layout_below="@+id/biller_avatar_image_view" layout_centerHorizontal="true" lineSpacingMultiplier="0.6" layout_marginStart="8dp" layout_marginEnd="8dp" style="@style/2131820992"/>
    </RelativeLayout>
    <RelativeLayout id="@+id/biller_list_layout" background="@drawable/ripple_rounded_purple" layout_width="match_parent" layout_height="wrap_content">
        <ImageView id="@+id/biller_avatar_image_view_list" layout_width="50dp" layout_height="50dp" layout_marginTop="8dp" layout_centerVertical="true" layout_marginStart="16dp"/>
        <TextView ellipsize="end" id="@+id/biller_name_text_view_list" layout_width="wrap_content" layout_height="wrap_content" layout_marginTop="8dp" maxLines="1" layout_alignParentTop="true" layout_marginStart="24dp" layout_marginEnd="16dp" layout_toEndOf="@+id/biller_avatar_image_view_list" style="@style/2131821015"/>
        <TextView id="@+id/biller_code_text_view_list" layout_width="wrap_content" layout_height="wrap_content" layout_marginTop="8dp" layout_below="@+id/biller_name_text_view_list" layout_marginStart="24dp" layout_marginEnd="16dp" layout_toEndOf="@+id/biller_avatar_image_view_list" style="@style/2131821085"/>
        <View id="@+id/biller_divider" background="#eae7ec" visibility="invisible" layout_width="match_parent" layout_height="1dp" layout_marginTop="8dp" layout_below="@+id/biller_code_text_view_list" layout_marginStart="80dp"/>
        <View id="@+id/biller_full_divider" background="#eae7ec" visibility="gone" layout_width="match_parent" layout_height="1dp" layout_marginTop="8dp" layout_below="@+id/biller_name_text_view_list"/>
    </RelativeLayout>
</FrameLayout>
