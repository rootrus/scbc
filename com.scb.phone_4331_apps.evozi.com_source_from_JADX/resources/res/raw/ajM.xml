    <RelativeLayout layout_width="0dp" layout_height="wrap_content" layout_marginTop="4dp" layout_weight="0.1">
        <ImageView id="@+id/legend_circle" layout_width="10dp" layout_height="10dp" src="@drawable/shape_circle_white_background" layout_centerInParent="true" contentDescription="@null"/>
    </RelativeLayout>
    <TextView id="@+id/asset_description_text_view" layout_width="0dp" layout_height="wrap_content" maxLines="2" layout_weight="0.65" paddingStart="4dp" style="@style/2131820865"/>
    <TextView gravity="right" id="@+id/asset_percent_text_view" layout_width="0dp" layout_height="wrap_content" layout_weight="0.25" style="@style/2131820865"/>
</LinearLayout>
