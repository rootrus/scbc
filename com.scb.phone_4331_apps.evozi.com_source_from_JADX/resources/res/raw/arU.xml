<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" orientation="vertical" background="#ffffff" layout_width="match_parent" layout_height="wrap_content">
    <View style="@style/2131821259"/>
    <TextView textColor="#736e78" id="@+id/label_costOverview" background="#f5f3f6" padding="16dp" layout_width="match_parent" layout_height="wrap_content" text="@string/cost_overview" style="@style/2131821058"/>
    <View style="@style/2131821259"/>
    <LinearLayout gravity="center_vertical" orientation="horizontal" layout_width="match_parent" layout_height="wrap_content">
        <TextView id="@+id/tvRateLabel" padding="16dp" layout_width="0dp" layout_height="wrap_content" text="@string/monthly_interest_rate_label" layout_weight="1" style="@style/2131821012"/>
        <TextView textColor="#353239" id="@+id/txv_monthly_interest_rate" layout_width="wrap_content" layout_height="wrap_content" layout_marginTop="16dp" layout_marginBottom="16dp" layout_marginEnd="16dp" NOT_FOUND_0x12="0" NOT_FOUND_0x13="0" style="@style/2131821020"/>
    </LinearLayout>
    <View style="@style/2131821260"/>
    <include id="@+id/layout_total_plan_details" layout="@layout/2131494362"/>
    <LinearLayout orientation="vertical" id="@+id/layout_monthly_plan_details" layout_width="match_parent" layout_height="wrap_content">
        <View style="@style/2131821260"/>
        <LinearLayout orientation="horizontal" layout_width="match_parent" layout_height="wrap_content">
            <TextView id="@+id/monthly_plan_details_label" padding="16dp" layout_width="0dp" layout_height="wrap_content" text="@string/monthly_payment" layout_weight="1" style="@style/2131821012"/>
            <TextView textColor="#353239" layout_gravity="center_vertical" id="@+id/txv_monthly_plan_details" layout_width="wrap_content" layout_height="wrap_content" layout_marginTop="16dp" layout_marginBottom="16dp" layout_marginEnd="16dp" style="@style/2131821020"/>
        </LinearLayout>
    </LinearLayout>
    <View style="@style/2131821259"/>
    <LinearLayout id="@+id/review_button_view" background="#f5f3f6" paddingTop="24dp" paddingBottom="24dp" layout_width="match_parent" layout_height="wrap_content" paddingStart="16dp" paddingEnd="16dp">
        <Button id="@+id/btn_deejung_review" layout_width="match_parent" text="@string/review" style="@style/2131820837"/>
    </LinearLayout>
</LinearLayout>
