<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" orientation="vertical" layout_width="match_parent" layout_height="wrap_content">
    <LinearLayout layout_width="match_parent" layout_height="wrap_content" layout_marginTop="16dp" paddingStart="16dp" paddingEnd="16dp">
        <TextView id="@+id/text_fund_code" layout_width="0dp" layout_height="wrap_content" layout_weight="1" style="@style/2131821015"/>
    </LinearLayout>
    <LinearLayout orientation="horizontal" layout_width="match_parent" layout_height="wrap_content" paddingStart="16dp" paddingEnd="16dp">
        <TextView id="@+id/text_nav" layout_width="0dp" layout_height="wrap_content" layout_weight="1" style="@style/2131821058"/>
        <TextView id="@+id/text_balance" layout_width="wrap_content" layout_height="wrap_content" drawablePadding="8dp" style="@style/2131821015"/>
    </LinearLayout>
    <LinearLayout gravity="end" layout_width="match_parent" layout_height="wrap_content" layout_marginEnd="8dp">
        <TextView id="@+id/text_gain" padding="8dp" layout_width="wrap_content" layout_height="wrap_content" style="@style/2131821070"/>
    </LinearLayout>
</LinearLayout>
